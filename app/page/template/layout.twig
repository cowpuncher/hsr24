{% import '../_macros/_svg.twig' as svg %}
<!DOCTYPE html>
<html>
  <head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{% block title %}Default page name{% endblock %}</title>
	<meta name="description" content="">
	<meta name="author" content="">

	<!-- Mobile Meta -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
	<!-- <meta name="viewport" content="width=device-width, target-densitydpi=device-dpi"> -->

	<!-- Prevent click-to-call by Skype -->
	<meta name="SKYPE_TOOLBAR" content ="SKYPE_TOOLBAR_PARSER_COMPATIBLE" />

	<!-- Prevent iOS Safary detect numbers as phone -->
	<meta name="format-detection" content="telephone=no">

	<!-- Favicon -->
	<link rel="shortcut icon" href="favicon.ico">

	<!-- build:styles_version -->
	<link rel="stylesheet" href="template_styles.css">
	  <!-- endbuild -->

    <!-- Перенес скрипты вверх для имитации работы на площадке разработки, потому что там они так же подключаются в head. Атрибут defer не блокирует страницу, а скрипты выполняются, когда дерево DOM готово-->
    <script src="https://code.jquery.com/jquery-3.0.0.min.js"></script>
    <script src="http://api-maps.yandex.ru/2.0/?apikey=380a270f-ad53-406b-8d61-f7cc553394c5&load=package.full&lang=ru-RU" type="text/javascript"></script>

    {# <script src="https://api-maps.yandex.ru/2.1/?apikey=<ваш API-ключ>&lang=ru_RU" type="text/javascript"></script> #}
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

	<script src="https://cdnjs.cloudflare.com/ajax/libs/Swiper/4.5.0/js/swiper.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/svg4everybody/2.1.9/svg4everybody.min.js"></script>

    <script src="js/libs.js" defer></script>
	<script src="js/main.js" defer></script>
  </head>

  <body class="{% block bodyClass %}{% endblock %}">

	<div class="page {% block pageClass %}{% endblock %}">
        <div class="preloader visible" id="preloader">
            <img src="assets/images/required/loader.gif" alt="">
        </div>
        {% include '../../components/header/header.twig' %}

	  <main>
		{% block content %}{% endblock %}
	  </main>

	  {% include '../../components/footer/footer.twig' %}
      {% include '../../components/popup/popup.twig' %}
	</div> <!-- page-wrapper end -->

	<!-- JavaScript files placed at the end of the document so the pages load faster
	================================================== -->

  </body>
</html>
